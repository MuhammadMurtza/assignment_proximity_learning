<div id="<%= dom_id employment %>" class="card text-center">
  <tr>
    <strong>Employer:</strong>
    <%= employment.employer %>
    <strong>Date started:</strong>
    <%= employment.start_date %>
    <strong>Date ended:</strong>
    <%= employment.end_date %>
    <% if editable %>
      <%= button_to "Edit", edit_employment_path(employment), method: :get, data: { turbo_frame: 'modal' },  class:"btn btn-outline-warning"%>
    <% end %>
  </tr>
</div>
